{
    "repositories": [
        {
            "type": "path",
            "url": "php-server-sdk-shared-tests"
        }
    ],
    "name": "launchdarkly/server-sdk-consul",
    "description": "LaunchDarkly PHP SDK Consul integration",
    "keywords": [
        "launchdarkly",
        "launchdarkly php"
    ],
    "homepage": "https://github.com/launchdarkly/php-server-sdk-consul",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "LaunchDarkly <sdks@launchdarkly.com>",
            "homepage": "http://www.launchdarkly.com/"
        }
    ],
    "require": {
        "php": ">=7.4",
        "friendsofphp/consul-php-sdk": "^5",
        "launchdarkly/server-sdk": "^4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "launchdarkly/server-sdk-shared-tests": "@dev"
    },
    "autoload": {
        "psr-4": {
            "LaunchDarkly\\": "src/LaunchDarkly/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaunchDarkly\\Impl\\Integrations\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
